<h1>Sign Up</h1>

<%= render partial: "layouts/errors", locals: {object: @farrier} %>

<%= form_for @farrier do |f| %>
  <%= f.label :company_name %>
  <%= f.text_field :company_name %>
  <br><br>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>
  <br><br>
  <%= f.label :last_name %>
  <%= f.text_field :last_name %>
  <br><br>
  <%= f.label :username %>
  <%= f.text_field :username %>
  <br><br>
  <%= f.label :email %>
  <%= f.email_field :email %>
  <br><br>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <br><br>
  <%= f.submit "Submit" %>
<% end %>