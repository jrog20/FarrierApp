<h1>Add a New Barn</h1>
<%= form_for @barn do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br><br>
  <%= f.label :manager_name %>
  <%= f.text_field :manager_name %>
  <br><br>
  <%= f.label :phone %>
  <%= f.text_field :phone %>
  <br><br>
  <%= f.label :email %>
  <%= f.email_field :email %>
  <br><br>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <br><br>
  <%= f.label :city %>
  <%= f.text_field :city %>
  <br><br>
  <%= f.label :state %>
  <%= f.text_field :state %>
  <br><br>
  <%= f.label :zip_code %>
  <%= f.text_field :zip_code %>
  <br><br>
  <%= f.label :comments %>
  <%= f.text_area :comments %>
  <br><br>
  <%= f.submit %>
<% end %>
