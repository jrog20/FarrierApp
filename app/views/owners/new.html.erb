<h1>Add a New Owner</h1>
<%= form_for @owner do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>
  <br><br>
  <%= f.label :last_name %>
  <%= f.text_field :last_name %>
  <br><br>
  <%= f.label :phone %>
  <%= f.text_field :phone %>
  <br><br>
  <%= f.label :email %>
  <%= f.email_field :email %>
  <br><br>
  <%= f.label :comments %>
  <%= f.text_area :comments %>
  <br><br>
  <%= f.submit %>
<% end %>